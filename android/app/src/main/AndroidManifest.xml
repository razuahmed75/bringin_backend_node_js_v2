               android:value="YOUR KEY HERE"/>
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            
            </intent-filter>
    <meta-data android:name="flutter_deeplinking_enabled" android:value="true" />
<!-- Deep Links -->
